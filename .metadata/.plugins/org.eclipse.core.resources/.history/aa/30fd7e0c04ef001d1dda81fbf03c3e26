	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>ServletStudy</display-name>

	<servlet>
		<servlet-name>MyfirstServlet</servlet-name>
		<servlet-class>edu.pnu.servlet.MyServlet01</servlet-class> <!--아까 만든 클래스 패키지 위치 가져와서 . 찍어 -->
	</servlet>
	<servlet-mapping>
		<servlet-name>MyfirstServlet</servlet-name>
		<url-pattern>/myservlet01</url-pattern> <!-- localhost:8080/MustHaveJSP/myservlet01 로 되게끔 설정. JSP/ 까진 자동. -->
	</servlet-mapping>
</web-app>